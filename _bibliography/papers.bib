	author = {Majewska, Roksana and Ashworth, Matt P. and Bosak, Sunčica and Goosen, William E. and Nolte, Christopher and Filek, Klara and Vijver, Bart Van de and Taylor, Jonathan C. and Manning, Schonna R. and Nel, Ronel},
	urldate = {2021-08-13},
	date = {2021-02-01},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/jpy.13086},
	keywords = {sea turtle, Craspedostauros, epizoic diatom, loggerhead, barnacle, Chelonibia, leatherback, phylogeny, Platylepas},
