Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {\textit{Bacillariophyta}, \textit{Caretta caretta}, epibiont, epizoic biofilm, marine gomphonemoid diatoms, Mediterranean Sea},
	file = {Full Text PDF:/home/lupo/Nextcloud/Zotero/storage/LBPCWWP9/Kanjer et al. - 2020 - Diatom Diversity on the Skin of Frozen Historic Lo.pdf:application/pdf;Snapshot:/home/lupo/Nextcloud/Zotero/storage/9SWVJ5XS/383.html:text/html}
