	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/jpy.13122},
	keywords = {{SEM}, diatom identification, diatom morphology, method, mounting medium, permanent slide, Proschkinia bulnheimii var. belgica, resin, taxonomy, type material},
	file = {Full Text PDF:/home/lupo/Nextcloud/Zotero/storage/SKX5567V/Majewska et al. - 2021 - Diatom type material in permanent slides does not .pdf:application/pdf;Snapshot:/home/lupo/Nextcloud/Zotero/storage/D2ARR89V/jpy.html:text/html}
