@article{malgieri_folding_2018,
	title = {Folding mechanisms steer the amyloid fibril formation propensity of highly homologous proteins.},
	volume = {9},
	issn = {2041-6539},
	url = {https://pubs.rsc.org/en/content/articlelanding/2018/sc/c8sc00166a},
	doi = {10.1039/C8SC00166A},
	abstract = {Significant advances in the understanding of the molecular determinants of fibrillogenesis can be expected from comparative studies of the aggregation propensities of proteins with highly homologous structures but different folding pathways. Here, we fully characterize, by means of stopped-flow, T-jump, {CD} and {DSC} experiments, the unfolding mechanisms of three highly homologous proteins, zinc binding Ros87 and Ml153–149 and zinc-lacking Ml452–151. The results indicate that the three proteins significantly differ in terms of stability and (un)folding mechanisms. Particularly, Ros87 and Ml153–149 appear to be much more stable to guanidine denaturation and are characterized by folding mechanisms including the presence of an intermediate. On the other hand, metal lacking Ml452–151 folds according to a classic two-state model. Successively, we have monitored the capabilities of Ros87, Ml452–151 and Ml153–149 to form amyloid fibrils under native conditions. Particularly, we show, by {CD}, fluorescence, {DLS}, {TEM} and {SEM} experiments, that after 168 hours, amyloid formation of Ros87 has started, while Ml153–149 has formed only amorphous aggregates and Ml452–151 is still monomeric in solution. This study shows how metal binding can influence protein folding pathways and thereby control conformational accessibility to aggregation-prone states, which in turn changes aggregation kinetics, shedding light on the role of metal ions in the development of protein deposition diseases.},
	pages = {3290--3298},
	number = {13},
	journaltitle = {Chemical Science},
	shortjournal = {Chem. Sci.},
	author = {Malgieri, Gaetano and D'Abrosca, Gianluca and Pirone, Luciano and Toto, Angelo and Palmieri, Maddalena and Russo, Luigi and Sciacca, Michele Francesco Maria and Tatè, Rosarita and Sivo, Valeria and Baglivo, Ilaria and Majewska, Roksana and Coletta, Massimo and Pedone, Paolo Vincenzo and Isernia, Carla and Stefano, Mario De and Gianni, Stefano and Pedone, Emilia Maria and Milardi, Danilo and Fattorusso, Roberto},
	urldate = {2021-08-13},
	date = {2018-03-28},
	langid = {english},
	note = {Publisher: The Royal Society of Chemistry},
	file = {Full Text PDF:/home/lupo/Nextcloud/Zotero/storage/3Z4NKPEJ/Malgieri et al. - 2018 - Folding mechanisms steer the amyloid fibril format.pdf:application/pdf;Snapshot:/home/lupo/Nextcloud/Zotero/storage/VYH2E8CH/C8SC00166A.html:text/html;Supplementary Information PDF:/home/lupo/Nextcloud/Zotero/storage/AS4QDZYU/Malgieri et al. - 2018 - Folding mechanisms steer the amyloid fibril format.pdf:application/pdf}
}

@article{majewska_comparison_2015,
	title = {A comparison of epiphytic diatom communities on \textit{Plocamium cartilagineum} (Plocamiales, Florideophyceae) from two Antarctic areas.},
